{
	"name": "NYCEmpMaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DataLakeEmpMaster",
						"type": "DatasetReference"
					},
					"name": "DataLakeEmpMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SQLDBNYCPayrollEMPMD",
						"type": "DatasetReference"
					},
					"name": "SQLDBNYCPayrollAGENCYMD"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as integer,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DataLakeEmpMaster",
				"DataLakeEmpMaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          AgencyID,",
				"          AgencyName",
				"     )) ~> SQLDBNYCPayrollAGENCYMD"
			]
		}
	}
}